@{
    ViewBag.Title = "Iniciar Procesos";
    var entero = Request.Url.PathAndQuery;
    var path = entero.Substring(entero.IndexOf('?') + 6);
    ViewBag.BLUP = path.Substring(0, path.Length - 14);
}

@*@MiHelper.DevuelveDatosUsuario(ViewBag.Resultado)*@
<input type="hidden" id="blupInitiate" value="~/Home/Index?BLUP=@ViewBag.BLUP&Aplicacion=MA" />
<input type="hidden" id="blup" value="~/Home/inbox?BLUP=@ViewBag.BLUP&Aplicacion=MA" />
<input type="hidden" id="blupCompleted" value="~/Home/completed?BLUP=@ViewBag.BLUP&Aplicacion=MA" />

